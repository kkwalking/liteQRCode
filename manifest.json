{
    "manifest_version": 3,
    "name": "网页二维码生成器",
    "version": "1.0",
    "description": "生成当前网页的二维码并显示在右下角",
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "qrcode.min.js",
                "content.js"
            ],
            "css": [
                "styles.css"
            ]
        }
    ],
    "action": {}
}
